{
    "name": "get_values_at_timestamp",
    "title": "Get Position at Timestamp",
    "description": "Retunr the interpolated positions at the time closest to the provided timestamp",
    "version": "1.0.0",
    "icon": "trajectory_intersection-1.svg",
    "cartoEnvVars": [],
    "inputs": [
        {
            "name": "input_table",
            "title": "Input table",
            "description": "Input table containing the trajectories",
            "type": "Table"
        },
        {
            "name": "traj_id_col",
            "title": "Trajectory ID",
            "description": "Column with the trajectory IDs",
            "type": "Column",
            "parent": "input_table",
            "noDefault": "true",
            "optional": false
        },
        {
            "name": "tpoints_col",
            "title": "Trajectory points",
            "description": "Column with the trajectory points",
            "type": "Column",
            "parent": "input_table",
            "noDefault": "true",
            "optional": false
        },
        {
            "name": "timestamp",
            "title": "Timestamp",
            "description": "The timestamp to get the position at",
            "type": "String",
            "placeholder": "YYYY-MM-DDTHH:MM:SSZ"
        }
    ],
    "outputs": [
        {
            "name": "output_table",
            "title": "Output table",
            "description": "The table with the column added",
            "type": "Table"
        }
    ]
}
